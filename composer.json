{
    "name": "hanoivip/user",
    "type": "library",
    "license": "MIT",
    "require": {
		"php" : ">=7.0.0",
		"guzzlehttp/guzzle" : "^6.3",
		"laravel/framework" : ">=5.5",
		"laravel/tinker" : "~1.0",
		"mervick/curl-helper" : "^2.2",
		"doctrine/dbal": "^2.13",
		"mews/captcha": "^2.1",
		"pragmarx/google2fa-laravel": "^2.0",
		"simplesoftwareio/simple-qrcode": "^2.0"
	},
	"autoload" : {
		"psr-4" : {
			"Hanoivip\\User\\" : "src/"
		},
		"files": [
			"src/helper.php"
		]
	},
	"extra" : {
